  <category name="Examples">
    <button text="SELECT *" callbackKey="example" query="SELECT *&#xD;&#xA;WHERE {&#xD;&#xA;	?s ?p ?o&#xD;&#xA;}"/>
    <button text="Class popularity" callbackKey="example" query="PREFIX rdfs: &lt;http://www.w3.org/2000/01/rdf-schema#&gt;&#xD;&#xA;&#xD;&#xA;SELECT ?class (COUNT(?resource) AS ?count)&#xD;&#xA;WHERE {&#xD;&#xA;	?resource a ?class .&#xD;&#xA;}&#xD;&#xA;GROUP BY ?class&#xD;&#xA;ORDER BY DESC(COUNT(?resource))&#xD;&#xA;LIMIT 10"/>
    <button text="Wikidata: Oscars/type" callbackKey="example" query="# See https://www.wikidata.org/wiki/Wikidata:SPARQL_query_service/queries/examples#Number_of_handed_out_academy_awards_per_award_type&#xD;&#xA;&#xD;&#xA;BASE &lt;http://www.wikidata.org/&gt;&#xD;&#xA;PREFIX p: &lt;prop/&gt;&#xD;&#xA;PREFIX pq: &lt;prop/qualifier/&gt;&#xD;&#xA;PREFIX ps: &lt;prop/statement/&gt;&#xD;&#xA;PREFIX wdt: &lt;prop/direct/&gt;&#xD;&#xA;PREFIX wd: &lt;entity/&gt;&#xD;&#xA;PREFIX bd: &lt;http://www.bigdata.com/rdf#&gt;&#xD;&#xA;PREFIX wikibase: &lt;http://wikiba.se/ontology#&gt;&#xD;&#xA;&#xD;&#xA;SELECT ?awardCount ?award ?awardLabel&#xD;&#xA;WHERE {&#xD;&#xA;	{&#xD;&#xA;		SELECT (COUNT(?award) AS ?awardCount) ?award&#xD;&#xA;		WHERE {&#xD;&#xA;			{&#xD;&#xA;				SELECT (SAMPLE(?human) AS ?human) ?award ?awardWork (SAMPLE(?director) AS ?director) (SAMPLE(?awardEdition) AS ?awardEdition) (SAMPLE(?time) AS ?time)&#xD;&#xA;				WHERE {&#xD;&#xA;					?award wdt:P31 wd:Q19020 .&#xD;&#xA;					{&#xD;&#xA;						?human&#xD;&#xA;							p:P166 ?awardStat ;&#xD;&#xA;							wdt:P31 wd:Q5 .&#xD;&#xA;						?awardStat&#xD;&#xA;							ps:P166 ?award ;&#xD;&#xA;							pq:P805 ?awardEdition ;&#xD;&#xA;							pq:P1686 ?awardWork&#xD;&#xA;					}&#xD;&#xA;					UNION&#xD;&#xA;					{&#xD;&#xA;						?awardWork&#xD;&#xA;							wdt:P31 wd:Q11424 ;&#xD;&#xA;							p:P166 ?awardStat .&#xD;&#xA;						?awardStat&#xD;&#xA;							ps:P166 ?award ;&#xD;&#xA;							pq:P805 ?awardEdition&#xD;&#xA;					} .&#xD;&#xA;					OPTIONAL {&#xD;&#xA;						?awardEdition wdt:P585 ?time .&#xD;&#xA;						?awardWork wdt:P57 ?director&#xD;&#xA;					}&#xD;&#xA;				}&#xD;&#xA;				GROUP BY ?awardWork ?award&#xD;&#xA;			}&#xD;&#xA;		}&#xD;&#xA;		GROUP BY ?award&#xD;&#xA;		ORDER BY ?awardCount&#xD;&#xA;	} .&#xD;&#xA;	SERVICE wikibase:label {&#xD;&#xA;		bd:serviceParam wikibase:language '[AUTO_LANGUAGE],en'&#xD;&#xA;	}&#xD;&#xA;}"/>
    <button text="UK Parliament: Procedure" callbackKey="example" query="# See https://github.com/ukparliament/Query/blob/master/Parliament.Data.Api.FixedQuery/Sparql/work_package_by_id.sparql&#xD;&#xA;&#xD;&#xA;PREFIX ex: &lt;https://example.com/&gt;&#xD;&#xA;PREFIX : &lt;http://id.parliament.uk/schema/&gt;&#xD;&#xA;&#xD;&#xA;CONSTRUCT {&#xD;&#xA;	?package&#xD;&#xA;		a :WorkPackage ;&#xD;&#xA;		:workPackageHasWorkPackageableThing ?workPackaged ;&#xD;&#xA;		:workPackageHasProcedure ?procedure ;&#xD;&#xA;		:workPackageHasBusinessItem ?item .&#xD;&#xA;	?workPackaged&#xD;&#xA;		a :WorkPackageableThing, ?class ;&#xD;&#xA;		:workPackageableThingName ?workPackagedThingName ;&#xD;&#xA;		:workPackageableThingHasWorkPackageableThingWebLink ?workPackagedLink .&#xD;&#xA;	?procedure&#xD;&#xA;		a :Procedure ;&#xD;&#xA;		:procedureName ?procedureName .&#xD;&#xA;	?item&#xD;&#xA;		a :BusinessItem ;&#xD;&#xA;		:businessItemHasProcedureStep ?step ;&#xD;&#xA;		:businessItemDate ?itemDate ;&#xD;&#xA;		:businessItemHasBusinessItemWebLink ?itemLink .&#xD;&#xA;	?step&#xD;&#xA;		a :ProcedureStep, ex:Actualized ;&#xD;&#xA;		:procedureStepName ?stepName ;&#xD;&#xA;		:procedureStepHasHouse ?house ;&#xD;&#xA;		:procedureStepHasBusinessItem ?item ;&#xD;&#xA;		ex:ledTo ?otherStep ;&#xD;&#xA;		ex:canLeadTo ?otherStep .&#xD;&#xA;	?enablingStep&#xD;&#xA;		ex:enables ?possibleStep ;&#xD;&#xA;		ex:canLeadTo ?possibleStep .&#xD;&#xA;	?possibleStep&#xD;&#xA;		a :ProcedureStep, ex:Possible ;&#xD;&#xA;		:procedureStepName ?possibleName ;&#xD;&#xA;		:procedureStepHasHouse ?possibleHouse .&#xD;&#xA;	?rootStep a ex:Root .&#xD;&#xA;	?house&#xD;&#xA;		a :House ;&#xD;&#xA;		:houseName ?houseName .&#xD;&#xA;	?possibleHouse&#xD;&#xA;		a :House ;&#xD;&#xA;		:houseName ?possibleHouseName&#xD;&#xA;}&#xD;&#xA;WHERE {&#xD;&#xA;	BIND (?work_package_id AS ?package) .&#xD;&#xA;	?package :workPackageHasProcedure ?procedure .&#xD;&#xA;	?procedure :procedureName ?procedureName .&#xD;&#xA;	OPTIONAL {&#xD;&#xA;		?package :workPackageHasWorkPackagedThing ?workPackaged .&#xD;&#xA;		?workPackaged a ?class .&#xD;&#xA;		FILTER ((?class = :StatutoryInstrumentPaper || ?class = :ProposedNegativeStatutoryInstrumentPaper) || ?class = :Treaty) .&#xD;&#xA;		OPTIONAL {&#xD;&#xA;			?workPackaged :workPackagedThingName ?workPackagedThingName&#xD;&#xA;		} .&#xD;&#xA;		OPTIONAL {&#xD;&#xA;			?workPackaged :workPackagedThingHasWorkPackagedThingWebLink ?workPackagedLink&#xD;&#xA;		}&#xD;&#xA;	} .&#xD;&#xA;	OPTIONAL {&#xD;&#xA;		{&#xD;&#xA;			?enablingStep :procedureStepHasBusinessItem/:businessItemHasWorkPackage ?package .&#xD;&#xA;			?possibleRoute&#xD;&#xA;				:procedureRouteHasProcedure ?procedure ;&#xD;&#xA;				:procedureRouteIsFromProcedureStep ?enablingStep ;&#xD;&#xA;				:procedureRouteIsToProcedureStep ?possibleStep .&#xD;&#xA;			?possibleStep :procedureStepName ?possibleName .&#xD;&#xA;			OPTIONAL {&#xD;&#xA;				?possibleStep :procedureStepHasHouse ?possibleHouse .&#xD;&#xA;				?possibleHouse :houseName ?possibleHouseName&#xD;&#xA;			} .&#xD;&#xA;			FILTER NOT EXISTS {&#xD;&#xA;				?possibleStep :procedureStepHasBusinessItem/:businessItemHasWorkPackage ?package&#xD;&#xA;			} .&#xD;&#xA;			FILTER NOT EXISTS {&#xD;&#xA;				?precludingRoute&#xD;&#xA;					:procedureRouteHasProcedure ?procedure ;&#xD;&#xA;					:procedureRouteIsToProcedureStep ?possibleStep ;&#xD;&#xA;					:precludedProcedureRouteIsPrecludedByProcedureStep/:procedureStepHasBusinessItem/:businessItemHasWorkPackage ?package&#xD;&#xA;			} .&#xD;&#xA;			FILTER NOT EXISTS {&#xD;&#xA;				?requiringRoute&#xD;&#xA;					:procedureRouteHasProcedure ?procedure ;&#xD;&#xA;					:requiredProcedureRouteIsRequiredByProcedureStep ?possibleStep .&#xD;&#xA;				FILTER NOT EXISTS {&#xD;&#xA;					?requiringRoute :procedureRouteIsToProcedureStep/:procedureStepHasBusinessItem/:businessItemHasWorkPackage ?package&#xD;&#xA;				}&#xD;&#xA;			}&#xD;&#xA;		}&#xD;&#xA;		UNION&#xD;&#xA;		{&#xD;&#xA;			?item :businessItemHasWorkPackage ?package .&#xD;&#xA;			?package :workPackageHasProcedure ?procedure .&#xD;&#xA;			OPTIONAL {&#xD;&#xA;				?item :businessItemDate ?itemDate&#xD;&#xA;			} .&#xD;&#xA;			OPTIONAL {&#xD;&#xA;				?item :businessItemHasBusinessItemWebLink ?itemLink&#xD;&#xA;			} .&#xD;&#xA;			?step&#xD;&#xA;				:procedureStepHasBusinessItem ?item ;&#xD;&#xA;				:procedureStepName ?stepName .&#xD;&#xA;			OPTIONAL {&#xD;&#xA;				?step :procedureStepHasHouse ?house .&#xD;&#xA;				?house :houseName ?houseName&#xD;&#xA;			} .&#xD;&#xA;			OPTIONAL {&#xD;&#xA;				?actualizedRoute&#xD;&#xA;					:procedureRouteHasProcedure ?procedure ;&#xD;&#xA;					:causedProcedureRouteIsCausedByProcedureStep|:allowedProcedureRouteIsAllowedByProcedureStep ?step ;&#xD;&#xA;					:procedureRouteIsToProcedureStep ?otherStep .&#xD;&#xA;				?otherStep :procedureStepHasBusinessItem/:businessItemHasWorkPackage ?package .&#xD;&#xA;				FILTER (?otherStep != ?step)&#xD;&#xA;			}&#xD;&#xA;		}&#xD;&#xA;		UNION&#xD;&#xA;		{&#xD;&#xA;			?rootStep :procedureStepHasBusinessItem/:businessItemHasWorkPackage ?package .&#xD;&#xA;			OPTIONAL {&#xD;&#xA;				?higherRoute&#xD;&#xA;					:procedureRouteHasProcedure ?procedure ;&#xD;&#xA;					(:causedProcedureRouteIsCausedByProcedureStep|:allowedProcedureRouteIsAllowedByProcedureStep)/:procedureStepHasBusinessItem/:businessItemHasWorkPackage ?package ;&#xD;&#xA;					:procedureRouteIsToProcedureStep ?rootStep&#xD;&#xA;			} .&#xD;&#xA;			FILTER (!BOUND(?higherRoute))&#xD;&#xA;		}&#xD;&#xA;	}&#xD;&#xA;}"/>
    <button text="EU ODP: Dataset with title" callbackKey="example" query="# See https://data.europa.eu/euodp/en/Sample_SPARQL_queries#ODP-Developerscorner-SampleSPARQLqueries-Retrievedatasetwithspecifictitle(eg.&apos;RegisterofCommissiondocuments&apos;)&#xD;&#xA;&#xD;&#xA;PREFIX dcat: &lt;http://www.w3.org/ns/dcat#&gt;&#xD;&#xA;PREFIX odp: &lt;http://data.europa.eu/euodp/ontologies/ec-odp#&gt;&#xD;&#xA;PREFIX dc: &lt;http://purl.org/dc/terms/&gt;&#xD;&#xA;PREFIX xsd: &lt;http://www.w3.org/2001/XMLSchema#&gt;&#xD;&#xA;PREFIX foaf: &lt;http://xmlns.com/foaf/0.1/&gt;&#xD;&#xA;&#xD;&#xA;SELECT ?Graph ?DatasetTitle ?Publisher ?DatasetURI&#xD;&#xA;WHERE {&#xD;&#xA;	GRAPH ?Graph {&#xD;&#xA;		?DatasetURI&#xD;&#xA;			a dcat:Dataset ;&#xD;&#xA;			dc:publisher ?Publisher ;&#xD;&#xA;			dc:title ?DatasetTitle .&#xD;&#xA;		FILTER REGEX(?DatasetTitle, 'Register of Commission documents', 'i')&#xD;&#xA;	}&#xD;&#xA;}&#xD;&#xA;LIMIT 10"/>
    <button text="GraphDB: SPARQL*" callbackKey="example" query="# See https://graphdb.ontotext.com/documentation/9.2/free/devhub/rdf-sparql-star.html&#xD;&#xA;&#xD;&#xA;PREFIX foaf: &lt;http://xmlns.com/foaf/0.1/&gt;&#xD;&#xA;PREFIX ex: &lt;http://example.com/&gt;&#xD;&#xA;&#xD;&#xA;SELECT ?p ?a ?c&#xD;&#xA;WHERE {&#xD;&#xA;	BIND (&lt;&lt; ?p foaf:age ?a &gt;&gt; AS ?t) .&#xD;&#xA;	?t ex:certainty ?c&#xD;&#xA;}"/>
    <button text="Virtuoso: Insert example" callbackKey="example" query="# See http://vos.openlinksw.com/owiki/wiki/VOS/VirtTipsAndTricksSPARQL11Insert#INSERT%20%28Informative%29%20Examples&#xD;&#xA;&#xD;&#xA;BASE &lt;http://example.com&gt;&#xD;&#xA;PREFIX xsd: &lt;http://www.w3.org/2001/XMLSchema#&gt;&#xD;&#xA;&#xD;&#xA;INSERT DATA {&#xD;&#xA;	GRAPH &lt;urn:sparql:tests:insert:informative&gt; {&#xD;&#xA;		&lt;#book1&gt;&#xD;&#xA;			&lt;http://purl.org/dc/elements/1.1/title&gt; 'Fundamentals of Compiler Design' ;&#xD;&#xA;			&lt;http://purl.org/dc/elements/1.1/date&gt; '1977-01-01T00:00:00-02:00'^^xsd:dateTime .&#xD;&#xA;		&lt;#book2&gt;&#xD;&#xA;			&lt;#price&gt; 42 ;&#xD;&#xA;			&lt;http://purl.org/dc/elements/1.1/title&gt; 'David Copperfield' ;&#xD;&#xA;			&lt;http://purl.org/dc/elements/1.1/creator&gt; 'Edmund Wells' ;&#xD;&#xA;			&lt;http://purl.org/dc/elements/1.1/date&gt; '1948-01-01T00:00:00-02:00'^^xsd:dateTime&#xD;&#xA;	}&#xD;&#xA;} ;&#xD;&#xA;&#xD;&#xA;INSERT DATA {&#xD;&#xA;	GRAPH &lt;urn:sparql:tests:insert:informative2&gt; {&#xD;&#xA;		&lt;#book4&gt; &lt;http://purl.org/dc/elements/1.1/title&gt; 'SPARQL 1.0 Tutorial'&#xD;&#xA;	}&#xD;&#xA;} ;&#xD;&#xA;&#xD;&#xA;INSERT {&#xD;&#xA;	GRAPH &lt;urn:sparql:tests:insert:informative2&gt; {&#xD;&#xA;		?book ?p ?v&#xD;&#xA;	}&#xD;&#xA;}&#xD;&#xA;WHERE {&#xD;&#xA;	GRAPH &lt;urn:sparql:tests:insert:informative&gt; {&#xD;&#xA;		?book &lt;http://purl.org/dc/elements/1.1/date&gt; ?date .&#xD;&#xA;		FILTER (?date &gt; '1970-01-01T00:00:00-02:00'^^xsd:dateTime) .&#xD;&#xA;		?book ?p ?v&#xD;&#xA;	}&#xD;&#xA;}"/>
