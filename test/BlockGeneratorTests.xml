PREFIX : <urn:default:>
PREFIX p: <urn:p:>

SELECT DISTINCT ?g (0 AS ?v2)

FROM :
FROM p:
FROM :r
FROM p:r
FROM NAMED <n>

WHERE {
	?s1
		?p1 ?o1, ?o2 ;
		?p2 ?o3 .

	?s2 ?p3 << ?es ?ep ?eo >> .

	FILTER EXISTS {
		?e1 ?e2 ?e3
	}
}
GROUP BY ?g
HAVING (0)
ORDER BY ?o
LIMIT 1
OFFSET 2
VALUES (?1 ?2) {
	(0 1)
	(2 UNDEF)
}
